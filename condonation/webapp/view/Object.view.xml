<mvc:View
 xmlns:core="sap.ui.core"
	controllerName="com.knpl.pragati.condonation.controller.Object"
	xmlns="sap.m"
	xmlns:mvc="sap.ui.core.mvc"
	>

 <Page busy="{oModelControl>/bBusy}"  busyIndicatorDelay="0" showNavButton="false" showHeader="true" >
        <headerContent>
            <Bar>
                <contentLeft>
                    <Breadcrumbs currentLocationText="{i18n>AddNewCondonation}">
                        <Link text="{i18n>worklistTitle}" press="onNavBack"/>
                    </Breadcrumbs>
                </contentLeft>
            </Bar>
        </headerContent>
        <content>

            <VBox width="100%" id="oVbxSmtTbl">
                 <!-- <core:Fragment id="Add"  fragmentName="com.knpl.pragati.condonation.view.subview.Add" type="XML" /> -->
            </VBox>
        </content>
        <footer>
            <Toolbar>
                <ToolbarSpacer/>
               
                <Button width="7%" text="{i18n>Cancel}" type="Ghost" press="onNavBack"/>
                 <Button width="7%" text="{i18n>Save}" type="Emphasized" press="onPressSave"/>
            </Toolbar>
        </footer>
    </Page>


</mvc:View>