{
  "_version": "1.12.0",
  "sap.app": {
    "id": "com.knpl.pragati.ContactPainter",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "1.0.0"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "resources": "resources.json",
    "ach": "ach",
    "sourceTemplate": {
      "id": "html5moduletemplates.basicSAPUI5ApplicationProjectModule",
      "version": "1.40.12"
    },
    "dataSources": {
      "mainService": {
        "uri": "/KNPL_PAINTER_API/api/v2/odata.svc/",
        "type": "OData",
        "settings": {
          "odataVersion": "2.0",
          "localUri": "localService/metadata.xml",
           "annotations": [ 
                        "annotation0"   ] 
        }
      },
      "uploadData": {
        "uri": "model/uploaditems.json",
        "type": "JSON"
      },
      "annotation0": {
                "type": "ODataAnnotation",
                "uri": "annotation/annotation.xml"
        }

    },
    "crossNavigation": {
      "inbounds": {
        "intent1": {
          "signature": {
            "parameters": {},
            "additionalParameters": "allowed"
          },
          "semanticObject": "Manage",
          "action": "CP",
          "title": "{{appTitle}}",
          "subTitle": "{{appSubTitle}}",
          "icon": "sap-icon://employee-pane"
        }
      }
    }
  },
  "sap.fiori": {
    "registrationIds": [],
    "archeType": "transactional"
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "flexEnabled": false,
    "rootView": {
      "viewName": "com.knpl.pragati.ContactPainter.view.App",
      "type": "XML",
      "async": true,
      "id": "App"
    },
    "dependencies": {
      "minUI5Version": "1.60.1",
      "libs": {
        "sap.ui.core": {},
        "sap.m": {},
        "sap.ui.layout": {},
        "sap.ushell": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "com.knpl.pragati.ContactPainter.i18n.i18n"
        }
      },
      "": {
        "dataSource": "mainService",
        "preload": true,
        "settings": {
          "defaultBindingMode": "TwoWay",
          "defaultCountMode": "Inline",
          "refreshAfterChange": true,
          "defaultUpdateMethod": "Put",
          "useBatch": false
        }
      },
      "uploadModel": {
        "type": "sap.ui.model.json.JSONModel"
      },
      "LoginInfo": {
        "type": "sap.ui.model.json.JSONModel"
    }
    },
    "services": {
			"ShellUIService": {
				"factoryName": "sap.ushell.ui5service.ShellUIService",
				"lazy": false,
				"settings": {
					"setTitle": "auto"
				}
			}
	},
    "resources": {
      "css": [
        {
          "uri": "css/style.css"
        }
      ]
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "viewType": "XML",
        "async": true,
        "viewPath": "com.knpl.pragati.ContactPainter.view",
        "controlAggregation": "pages",
        "controlId": "app",
        "clearControlAggregation": false
      },
      "routes": [
        {
          "name": "RoutePList",
          "pattern": "",
          "target": ["TargetPList"]
        },
        {
          "name": "RouteAddEditP",
          "pattern": "PainterAdd",
          "target": ["TargetRouteAddEditP"]
        },
        {
          "name": "RouteProfile",
          "pattern": "Profile/{mode}/{prop}",
          "target": ["TargetProfile"]
        },
        {
          "name": "RouteAdditionalRequestDetail",
          "pattern": "AdditionalRequestDetail/{Id}/{Pid}",
          "target": ["TargetAdditionalRequestDetail"]
        }
      ],
      "targets": {
        "TargetPList": {
          "viewType": "XML",
          "transition": "slide",
          "clearControlAggregation": false,
          "viewId": "PainterList",
          "viewName": "PainterList"
        },
        "TargetRouteAddEditP": {
          "viewType": "XML",
          "transition": "slide",
          "clearControlAggregation": false,
          "viewId": "AddEditPainter",
          "viewName": "AddEditPainter"
        },
        "TargetProfile": {
          "viewType": "XML",
          "transition": "slide",
          "clearControlAggregation": false,
          "viewId": "ProfilePainter",
          "viewName": "ProfilePainter"
        },
        "TargetAdditionalRequestDetail": {
          "viewType": "XML",
          "transition": "slide",
          "clearControlAggregation": false,
          "viewId": "AdditionalRequestDetail",
          "viewName": "AdditionalRequestDetail"
        }
      }
    }
  }
}
