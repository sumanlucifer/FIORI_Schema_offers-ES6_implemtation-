<core:FragmentDefinition xmlns:commons="sap.suite.ui.commons" xmlns="sap.m" xmlns:smartTable="sap.ui.comp.smarttable" xmlns:core="sap.ui.core">
    <Dialog title="{i18n>NameChangeHistory}" titleAlignment="Center" >
        <content>
             <IconTabBar expandable="false" expanded="true" id="iconTabBar"  class="sapUiResponsiveContentPadding"  select="onIcnTabChange" selectedKey="{oModelControl>/IcnTabKey}" >
                <items>
                    
                        <IconTabFilter  text="{i18n>History}" key="0" >
                                <smartTable:SmartTable
                            id="smartHistoryMobileChange"
                            editable="false"
                            beforeRebindTable="onBeforeMobileChangeHistory"                    
                            entitySet="PainterMobileLogSet"
                            tableType="ResponsiveTable"
                            showRowCount="true"
                            useExportToExcel="false"
                            useVariantManagement="false"
                            useTablePersonalisation="true"
                            demandPopin="true"
                            enableAutoBinding="true"
                            placeToolbarInTable="true"
                            persistencyKey="HistoryPersistenceKey"
                            
                             initialNoDataText="{i18n>tableNoDataText}"
                           >

        <Table growing="true" growingThreshold="10" growingScrollToLoad="false">
            <columns>
                <!-- <Column hAlign="Begin">
                    <customData>
                        <core:CustomData key="p13nData" value='\{"columnKey": "ComplaintCode", "leadingProperty": "ComplaintCode", "columnIndex": 0 ,"sortProperty": "ComplaintCode","filterProperty": "ComplaintCode"}'/>
                    </customData>
                    <Text text="{i18n>CondonationId}"/>
                </Column> -->
                 <Column hAlign="Begin">
                    <customData>
                        <core:CustomData key="p13nData" value='\{"columnKey": "UpdatedAt", "leadingProperty": "UpdatedAt", "columnIndex": 1 ,"sortProperty": "UpdatedAt","filterProperty": "UpdatedAt"}'/>
                    </customData>
                    <Text text="{i18n>UpdatedAt}"/>
                </Column>
                 <Column hAlign="Begin">
                    <customData>
                        <core:CustomData key="p13nData" value='\{"columnKey": "Status", "leadingProperty": "Status", "columnIndex": 2 ,"sortProperty": "Status","filterProperty": "Status"}'/>
                    </customData>
                    <Text text="{i18n>Status}"/>
                </Column>
                 <!-- <Column hAlign="Begin">
                    <customData>
                        <core:CustomData key="p13nData" value='\{"columnKey": "Remark", "leadingProperty": "Remark", "columnIndex": 3 ,"sortProperty": "Remark","filterProperty": "Remark"}'/>
                    </customData>
                    <Text text="{i18n>Remark}"/>
                </Column> -->
                  <!-- <Column hAlign="Begin">
                    <customData>
                        <core:CustomData key="p13nData" value='\{"columnKey": "UpdatedBy", "leadingProperty": "UpdatedByName", "columnIndex": 4 ,"sortProperty": "UpdatedByName","filterProperty": "UpdatedByName"}'/>
                    </customData>
                    <Text text="{i18n>UpdatedBy}"/>
                </Column> -->
               
                

            </columns>
            <items>
                <ColumnListItem>
                    <cells>
                    <!-- <HBox>
                    
                        <ObjectAttribute
		
			text="{ComplaintCode}"
				active="{= ${oModelControl>/ComplainCode}!== ${ComplaintCode} }"
				press="onPressHistoryCompliantCode" />
                    </HBox> -->
                        	
                       <Text text="{path:'UpdatedAt',type:'sap.ui.model.type.Date',formatOptions:{pattern:'dd/MM/YYYY h:mm a'}}" />
                         <Text text="{path:'Status',formatter:'.formatter.fmtStatus1'}"/>
                           <!-- <Text text="{Remark}"/> -->
                            <!-- <Text text="{UpdatedByName}"/> -->


                    </cells>
                </ColumnListItem>
            </items>
        </Table>
    </smartTable:SmartTable>
                        </IconTabFilter>
                        <IconTabFilter  text="{i18n>ExecutionLog}" key="1" >
                            <commons:Timeline
            height="100%"
			id="idTimeline"
            alignment="Right"
            scrollingFadeout="None"
            axisOrientation="Vertical"
			enableDoubleSided="false"
            forceGrowing="false"
            enableBusyIndicator= "true"
            enableModelFilter= "true"
			growingThreshold="100"
			filterTitle="Task Type"
            showIcons="true"
            showSearch="false"
            enableScroll="false"
            sortOldestFirst="false"
			content="{wfmodel1>/}">
		<commons:content>
           <!-- formatter required datetime, title icon -->
			<commons:TimelineItem	dateTime="{ path :'wfmodel>timestamp', formatter :'.formatter.ExecutionLogDateTime' }"
					title="{ parts :['wfmodel>subject' , 'wfmodel>type' ], formatter : '.formatter.ExecutionLogTitle'}"
					text="{ path : 'wfmodel>recipientUsers', formatter : '.formatter.ExecutionLogUserName' }"
                    filterValue="{wfmodel>type}"
					icon="{path : 'wfmodel>type' , formatter : '.formatter.formatLogIcon'}">
			</commons:TimelineItem>
		</commons:content>
	</commons:Timeline>
                        </IconTabFilter>
                    </items>
                </IconTabBar>
        </content>
        <endButton>
            <Button type="Ghost" text="Close" press=".onDialogCloseNew"/>
        </endButton>
    </Dialog>
</core:FragmentDefinition>