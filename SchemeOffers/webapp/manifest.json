{
    "_version": "1.12.0",
    "sap.app": {
        "id": "com.knpl.pragati.SchemeOffers",
        "type": "application",
        "i18n": "i18n/i18n.properties",
        "applicationVersion": {
            "version": "1.0.0"
        },
        "title": "{{appTitle}}",
        "description": "{{appDescription}}",
        "resources": "resources.json",
        "ach": "ach",
        "sourceTemplate": {
            "id": "html5moduletemplates.basicSAPUI5ApplicationProjectModule",
            "version": "1.40.12"
        },
        "crossNavigation": {
            "inbounds": {
                "intent1": {
                    "signature": {
                        "parameters": {},
                        "additionalParameters": "allowed"
                    },
                    "semanticObject": "Manage",
                    "action": "SM",
                    "title": "{{appTileTitle}}",
                    "subTitle": "{{appTileSubTitle}}",
                    "icon": "sap-icon://retail-store-manager"
                }
            }
        },
        "dataSources": {
            "mainService": {
                "uri": "/KNPL_PAINTER_API/api/v2/odata.svc/",
                "type": "OData",
                "settings": {
                    "odataVersion": "2.0",
                    "localUri": "localService/metadata.xml"
                }
            },
            "mockData_alias": {
                "uri": "model/data.json",
                "type": "JSON"
            },
            "fragment_JSON": {
                "uri": "model/local.json",
                "type": "JSON"
            }
        }
    },
    "sap.ui": {
        "technology": "UI5",
        "icons": {
            "icon": "",
            "favIcon": "",
            "phone": "",
            "phone@2": "",
            "tablet": "",
            "tablet@2": ""
        },
        "deviceTypes": {
            "desktop": true,
            "tablet": true,
            "phone": true
        }
    },
    "sap.ui5": {
        "flexEnabled": false,
        "rootView": {
            "viewName": "com.knpl.pragati.SchemeOffers.view.App",
            "type": "XML",
            "async": true,
            "id": "app"
        },
        "dependencies": {
            "minUI5Version": "1.60.1",
            "libs": {
                "sap.ui.core": {},
                "sap.m": {},
                "sap.ui.layout": {}
            }
        },
        "contentDensities": {
            "compact": true,
            "cozy": true
        },
        "models": {
            "i18n": {
                "type": "sap.ui.model.resource.ResourceModel",
                "settings": {
                    "bundleName": "com.knpl.pragati.SchemeOffers.i18n.i18n"
                }
            },
            "": {
                "dataSource": "mainService",
                "preload": true,
                "settings": {
                    "defaultBindingMode": "TwoWay",
                    "defaultCountMode": "Inline",
                    "refreshAfterChange": true,
                    "defaultUpdateMethod": "Put",
                    "useBatch": false
                }
            },
            "MockData": {
                "type": "sap.ui.model.json.JSONModel",
                "dataSource": "mockData_alias"
            },
            "LoginInfo": {
                "type": "sap.ui.model.json.JSONModel"
            },
            "FragData": {
                "type": "sap.ui.model.json.JSONModel",
                "dataSource": "fragment_JSON"
            }
        },
        "resources": {
            "css": [
                {
                    "uri": "css/style.css"
                }
            ]
        },
        "routing": {
            "config": {
                "routerClass": "sap.m.routing.Router",
                "viewType": "XML",
                "async": true,
                "viewPath": "com.knpl.pragati.SchemeOffers.view",
                "controlAggregation": "pages",
                "controlId": "app"
            },
            "routes": [
                {
                    "name": "RouteLandingPage",
                    "pattern": "",
                    "target": [
                        "TargetLandingPage"
                    ]
                },
                {
                    "name": "DetailPage",
                    "pattern": "Details/{prop}/{mode}",
                    "target": [
                        "TargetDetailPage"
                    ]
                },
                {
                    "name": "AddOfferPage",
                    "pattern": "Add",
                    "target": [
                        "TargetAddOfferPage"
                    ]
                }
            ],
            "targets": {
                "TargetLandingPage": {
                    "viewType": "XML",
                    "transition": "slide",
                    "viewName": "LandingPage",
                    "viewLevel": 1,
                    "viewId": "landingPage"
                },
                "TargetDetailPage": {
                    "viewType": "XML",
                    "transition": "slide",
                    "viewName": "DetailPage",
                    "viewLevel": 1,
                    "viewId": "detailPage"
                },
                "TargetAddOfferPage": {
                    "viewType": "XML",
                    "transition": "slide",
                    "viewName": "AddOfferPage",
                    "viewLevel": 1,
                    "viewId": "addOfferPage"
                }
            }
        }
    }
}