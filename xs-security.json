{
  "xsappname": "knpl-influencer-web",
  "tenant-mode": "dedicated",
  "oauth2-configuration": {
      "redirect-uris": [
          "https://*.ap10.applicationstudio.cloud.sap/login/callback",
          "https://*.cfapps.ap10.hana.ondemand.com/login/callback"
      ]
  },
  "description": "Security profile of called application",
  "scopes": [
    {
      "name": "uaa.user",
      "description": "UAA"
    },
    {
        "name": "$XSAPPNAME.Callback",
        "description": "With this scope set, the callbacks for tenant onboarding, offboarding and getDependencies can be called.",
        "grant-as-authority-to-apps": ["$XSAPPNAME(application, sap-provisioning, tenant-onboarding)"]
    }
  ],
  "authorities": [
      "$XSAPPNAME.Callback"
  ],
  "role-templates": [
    {
      "name": "Token_Exchange",
      "description": "UAA",
      "scope-references": [
        "uaa.user"
      ]
    }
  ]
}
